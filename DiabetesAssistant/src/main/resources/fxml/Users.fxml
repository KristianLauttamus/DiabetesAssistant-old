<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.*?>

<HBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.lauttadev.diabetesassistant.gui.UsersController">
   <children>
      <VBox alignment="CENTER" fillWidth="false" prefHeight="400.0" prefWidth="300.0">
         <children>
            <Label alignment="CENTER" text="Luo uusi käyttäjä" textAlignment="CENTER">
               <font>
                  <Font name="KaushanScript-Regular" size="20.0" />
               </font>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets right="5.0" />
               </padding>
            </Label>
                <TextField fx:id="nameTextField" alignment="CENTER" prefHeight="25.0" prefWidth="194.0" promptText="Käyttäjän nimi">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin></TextField>
                <Button fx:id="createUserBtn" mnemonicParsing="false" onAction="#handleCreateUserClick" prefHeight="25.0" prefWidth="77.0" text="Luo">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
      <VBox alignment="CENTER_LEFT" prefHeight="400.0" prefWidth="300.0">
         <children>
            <Label contentDisplay="CENTER" prefHeight="17.0" prefWidth="145.0" text="Valitse käyttäjä" textAlignment="JUSTIFY" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font name="KaushanScript-Regular" size="16.0" />
               </font>
               <padding>
                  <Insets right="4.0" />
               </padding>
            </Label>
            <ListView id="users" fx:id="users" prefHeight="360.0" prefWidth="296.0">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="One" />
                        <String fx:value="Two" />
                        <String fx:value="Three" />
                        <String fx:value="Four" />
                        <String fx:value="Five" />
                    </FXCollections>
                </items>
               <VBox.margin>
                  <Insets right="6.0" />
               </VBox.margin>
            </ListView>
         </children>
      </VBox>
   </children>
</HBox>
